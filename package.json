    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-express-graphql.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
